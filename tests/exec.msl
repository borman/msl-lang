fun sqr X
  return X*X
end

fun gcd [A, B]
  if B > A then
    [A, B] = [B, A]
  end
  while B > 0
    [A, B] = [B, A%B]
  end
  return A
end

fun ds [A, B]
  return [A+B, A-B]
end

